{
  "name": "adonis-hotwire",
  "version": "1.0.1",
  "private": false,
  "keywords": [
    "adonis",
    "hotwire",
    "deriegle",
    "turbolinks",
    "turbo streams",
    "stimulus"
  ],
  "files": [
    "build/adonis-typings",
    "build/commands",
    "build/providers",
    "build/templates",
    "build/instructions.js",
    "build/instructions.md"
  ],
  "typings": "./build/adonis-typings/index.d.ts",
  "scripts": {
    "mrm": "mrm --preset=@adonisjs/mrm-preset",
    "clean": "rimraf build",
    "build": "npm run lint && npm run clean && tsc && npm run copyfiles",
    "copyfiles": "copyfiles \"templates/**/*.txt\" \"instructions.md\" build",
    "lint": "eslint . --ext=.ts"
  },
  "devDependencies": {
    "@adonisjs/assembler": "^5.3.0",
    "@adonisjs/mrm-preset": "^4.0.1",
    "@symfony/webpack-encore": "^1.3.0",
    "adonis-preset-ts": "^2.1.0",
    "copyfiles": "^2.4.1",
    "eslint": "^7.27.0",
    "eslint-plugin-adonis": "^1.3.1",
    "pino-pretty": "^5.0.1",
    "rimraf": "^3.0.2",
    "typescript": "^4.2.4",
    "youch": "^2.2.2",
    "youch-terminal": "^1.1.1"
  },
  "dependencies": {
    "@adonisjs/core": "^5.1.7",
    "@adonisjs/lucid": "^18.0.0",
    "@adonisjs/view": "^6.0.2",
    "@hotwired/turbo": "^7.0.0-beta.5",
    "@poppinss/utils": "^3.1.3",
    "luxon": "^1.27.0",
    "proxy-addr": "^2.0.6",
    "reflect-metadata": "^0.1.13",
    "source-map-support": "^0.5.19",
    "sqlite3": "^5.0.2",
    "stimulus": "^2.0.0"
  },
  "adonisjs": {
    "instructions": "./build/instructions.js",
    "instructionsMd": "./build/instructions.md",
    "types": "adonis-hotwire",
    "providers": [
      "adonis-hotwire"
    ],
    "commands": [
      "adonis-hotwire/build/commands"
    ]
  }
}
